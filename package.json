    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "knex-mock-client": "^1.11.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
